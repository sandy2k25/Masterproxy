features:
  - node lts
nginx:
  root: public_html/dist/public
  passenger:
    enabled: "on"
    app_start_command: env PORT=$PORT node /home/heavy/public_html/dist/index.js
    app_env: production
commands:
  - ls -la
  - cat package.json
  - npm install --production
  - npm run build
  - ls -la dist/